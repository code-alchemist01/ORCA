{
  "name": "web-app",
  "replicas": 2,
  "strategy": "RollingUpdate",
  "container": {
    "name": "web-app",
    "image": "nginx:latest",
    "ports": {
      "80": "8080"
    },
    "environment": {
      "NGINX_HOST": "0.0.0.0",
      "NGINX_PORT": "80"
    },
    "volumes": [],
    "labels": {
      "app": "web-app",
      "tier": "frontend"
    }
  }
}